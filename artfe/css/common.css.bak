body {
    font-family: "Pretendard Variable", sans-serif;
    word-break: keep-all;
	padding: 0;
	margin: 0 auto;
}

img {
    display: block;
}

.page-title {
    font-size:50px;
    font-weight:800;
    line-height: 1.3;
    color:#514FF0;
    text-align: center;
    margin-bottom: 20px;
}

.page-subtitle {
    width:240px;
    height:50px;
    border: 5px solid #5bb451;
    border-radius:30px;
    display:flex;
    justify-content: center;
    align-items:center;
    font-size:24px;
    font-weight:700;
    color:#5bb451;
    margin-left:auto;
    margin-right:auto;
    margin-bottom: 20px;
}

.btn {
    max-width: 500px;
    height:100px;
    display:flex;
    justify-content: center;
    align-items: center;
    font-size: 50px;
    font-weight: 700;
    text-decoration: none;
    cursor:pointer;
    border-radius: 20px;
    margin-bottom:35px;
    margin-left:auto;
    margin-right:auto;
    transition: 0.1s;
}

.btn-green {
    background-color: #5bb451;
    box-shadow: 0 14px 0 #367d2e;
    color: #FFFFFF;
}


.btn-green:active {
    box-shadow: 0 4px 0 #367d2e;
    transform: translateY(10px);
}

.btn-purple {
    background-color: #514FF0;
    box-shadow: 0 14px 0 #3a38b5;
    color: #FFFFFF;
}

.btn-purple:active {
    box-shadow: 0 4px 0 #3a38b5;
    transform: translateY(10px);
}

.btn-gray {
    background-color: #F2F4F6;
    box-shadow: 0 14px 0 #DFE1E2;
    color:#444444;
}

.btn-gray:active {
    box-shadow: 0 4px 0 #DFE1E2;
    transform: translateY(10px);
}

.btn-small {
    max-width: 350px;
    height:70px;
    font-size:26px;
    margin-bottom: 30px;
}


@media (max-width : 640px) {
    body {
        padding-top : 50px;
        margin-left: 20px;
        margin-right: 20px;
	}

    .page-title {
        font-size:40px;
    }

    .btn {
        font-size: 34px;
    }

}

*{
	padding: 0;
	margin: 0 auto;
}
li {
	list-style-type: none;
}
img {
	border: 0;
}
a {
	color: black;
	text-decoration: none;
}
a:hover {
	color: #black;
	text-decoration: none;
}
#wrap {
	width:100%;
}
#wrap footer {
	width:90%;
	height: 100px;
	font-size: 1.5em;
	border: 1px solid #CCCCCC;
	font-weight: bold;
	line-height: 100px;
	margin-left:auto;
	margin-right:auto;
}

#wrap footer h6{
	font-size:16px;
	float:left;
	margin-left:30px;
}

#wrap footer a{
	float:right;
	margin-right:30px;
	font-size:13px;
}